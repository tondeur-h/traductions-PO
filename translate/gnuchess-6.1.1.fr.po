#, c-format
msgid "book is now on\n"
msgstr "La librairie est active\n"

#: src/frontend/cmd.cc:150
#, c-format
msgid "book is now off\n"
msgstr "La librairie est inactive\n"

#: src/frontend/cmd.cc:153
#, c-format
