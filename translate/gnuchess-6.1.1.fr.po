msgstr "La librairie est la meilleure\n"

#: src/frontend/cmd.cc:156
#, c-format
msgid "book is now worst\n"
msgstr "La librairie est la plus mauvaise\n"

#: src/frontend/cmd.cc:159
#, c-format
msgid "book is now random\n"
msgstr "La librairie est aléatoire\n"

#: src/frontend/cmd.cc:161
#, c-format
msgid "Incorrect book option\n"
msgstr "Option de la librairie incorrecte\n"

#: src/frontend/cmd.cc:174
#, c-format
msgid "Search to a depth of %d\n"
msgstr "Recherche à une profondeur de %d\n"

#: src/frontend/cmd.cc:240
#, c-format
msgid "Hashing is on.\n"
msgstr "Le hachage est actif.\n"

#: src/frontend/cmd.cc:242
#, c-format
